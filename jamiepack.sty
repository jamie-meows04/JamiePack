\newcommand{\ttW}{\tt{W}}
\newcommand{\ttX}{\tt{X}}
\newcommand{\ttY}{\tt{Y}}
\newcommand{\ttZ}{\tt{Z}}

% Environments

\makeatletter
\newcounter{statement}
\newcounter{exercise}

\ifx\claimsunder\empty\else
\counterwithin{statement}{\claimsunder}
\counterwithin{exercise}{\claimsunder}
\fi

\newcommand{\@claim}[2][]{\par\bigskip\noindent\refstepcounter{statement}\textbf{#2 \thestatement.} \quad \ifthenelse{\equal{#1}{}}{}{(#1)\par}}
\newcommand{\@@claim}[2][]{\par\bigskip\noindent\textbf{#2.} \quad \ifthenelse{\equal{#1}{}}{}{(#1)\par}}
\newcommand{\claim}{\@ifstar{\@@claim}{\@claim}}

\newcommand{\theorem}{\@ifstar{\@@theorem}{\@theorem}}
\newcommand{\@theorem}[1][] { \claim[#1]{Theorem} }
\newcommand{\@@theorem}[1][] { \claim*[#1]{Theorem} }

\newcommand{\proposition}{\@ifstar{\@@proposition}{\@proposition}}
\newcommand{\@proposition}[1][] { \claim[#1]{Proposition} }
\newcommand{\@@proposition}[1][] { \claim*[#1]{Proposition} }

\newcommand{\lemma}{\@ifstar{\@@lemma}{\@lemma}}
\newcommand{\@lemma}[1][] { \claim[#1]{Lemma} }
\newcommand{\@@lemma}[1][] { \claim*[#1]{Lemma} }

\newcommand{\corollary}{\@ifstar{\@@corollary}{\@corollary}}
\newcommand{\@corollary}[1][] { \claim[#1]{Corollary} }
\newcommand{\@@corollary}[1][] { \claim*[#1]{Corollary} }

\newcommand{\definition}{\@ifstar{\@@definition}{\@definition}}
\newcommand{\@definition}[1][] { \claim[#1]{Definition} }
\newcommand{\@@definition}[1][] { \claim*[#1]{Definition} }

\newcommand{\example}{\@ifstar{\@@example}{\@example}}
\newcommand{\@example}[1][] { \claim[#1]{Example} }
\newcommand{\@@example}[1][] { \claim*[#1]{Example} }

\newcommand{\remark}{\@ifstar{\@@remark}{\@remark}}
\newcommand{\@remark}[1][] { \claim[#1]{Remark} }
\newcommand{\@@remark}[1][] { \claim*[#1]{Remark} }

\newcommand{\exercise}{\refstepcounter{exercise}\par\bigskip
	\noindent\textbf{Exercise \theexercise.} \quad}

\renewcommand{\proof}[1][Proof] {\par\smallskip\noindent\textit{#1.}\quad}
\newcommand{\solution}{\proof[Solution]}

% Equations, figures, tables and captions

\newcommand{\eqlabel}[1]{\refstepcounter{statement}\tag{\thestatement}\label{#1}}

\let\oldfigure\figure
\renewcommand{\figure}{\oldfigure[ht]\centering\refstepcounter{statement}}
\newcommand{\figcaption}{\par\smallskip\textbf{Figure \thestatement.}\ }
\newcommand{\tabcaption}{\par\smallskip\textbf{Table \thestatement.}\ }

\let\oldprintbibliography\printbibliography
\renewcommand{\printbibliography}[1][]{\addcontentsline{toc}{section}{References} \oldprintbibliography[#1]}

\makeatother